<?xml version="1.0" encoding="UTF-8"?>

<FindBugsFilter xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xmlns="http://findbugs.sourceforge.net/filter/3.0.0"
  xsi:schemaLocation="http://findbugs.sourceforge.net/filter/3.0.0 https://findbugs.googlecode.com/git/findbugs/etc/findbugsfilter.xsd">

  <Match>
    <Or>
      <Package name="~com\.redknee\.topup\.alarms.*"/>
      <Package name="~com\.redknee\.unified_charging.*"/>
    </Or>
  </Match>

  <Match>
    <Field type="java.util.Map"/>
    <Bug pattern="SE_BAD_FIELD"/>
  </Match>
  <Match>
    <Field type="java.util.Set"/>
    <Bug pattern="SE_BAD_FIELD"/>
  </Match>
  <Match>
    <Field type="java.util.List"/>
    <Bug pattern="SE_BAD_FIELD"/>
  </Match>
  <Match>
    <Field type="java.security.Principal"/>
    <Bug pattern="SE_BAD_FIELD"/>
  </Match>
  <Match>
    <Field type="de.siemens.advantage.platform.cframe.intf.fapi.IFrwConfigDataSupport"/>
    <Bug pattern="SE_BAD_FIELD"/>
  </Match>
  <Match>
    <Field type="javax.transaction.UserTransaction"/>
    <Bug pattern="SE_BAD_FIELD"/>
  </Match>
  <Match>
    <Field type="javax.persistence.EntityManager"/>
    <Bug pattern="SE_BAD_FIELD"/>
  </Match>

</FindBugsFilter>